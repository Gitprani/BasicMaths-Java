class Solution {
    public int reverse(int x) {
          int org=0;
        while(x!=0){
          int rem=x%10;
          
          if(org>Integer.MAX_VALUE/10 || org<Integer.MIN_VALUE/10){
            return 0;

          }
           org=org*10+rem;
           x/=10;
        }
        return org;
    }
}
