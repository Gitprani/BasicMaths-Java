public static boolean isPrime(int n){                                                                  TimeComplexity = O(Sqrt(n)
	    int count=0;
	    
	    for(int i=1;i*i<=n;i++){    // check upto square root to reduce complexity
	        if(n%i==0){
	            count++;
	            if((n/i)!=i){
	                count++;
	            }
	        }
	        
	        
	    }
	    return (count==2);
	    
	    
	    
	}
